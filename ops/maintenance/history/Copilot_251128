# backend/app/schemas/user.py
from pydantic import field_validator
import re

@field_validator('password')
@classmethod
def validate_password(cls, v):
    if len(v) < 10:
        raise ValueError('ìµœì†Œ 10ì ì´ìƒ')
    
    # ìµœì†Œ 3ê°€ì§€ ë¬¸ì ìœ í˜• í•„ìš”
    categories = sum([
        bool(re.search(r'[A-Z]', v)),  # ëŒ€ë¬¸ì
        bool(re.search(r'[a-z]', v)),  # ì†Œë¬¸ì
        bool(re.search(r'\d', v)),      # ìˆ«ì
        bool(re.search(r'[^A-Za-z0-9]', v))  # íŠ¹ìˆ˜ë¬¸ì
    ])
    
    if categories < 3:
        raise ValueError('ëŒ€ë¬¸ì, ì†Œë¬¸ì, ìˆ«ì, íŠ¹ìˆ˜ë¬¸ì ì¤‘ 3ê°€ì§€ ì´ìƒ í¬í•¨')
    
    return v

