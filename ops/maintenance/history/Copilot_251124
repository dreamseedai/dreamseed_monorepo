- Student Dashboard (student_front):

// abilities.ts
export async function getAbilitySummary(token: string) {
  const res = await fetch('/api/abilities/me/summary', {
    headers: { Authorization: `Bearer ${token}` }
  });
  return res.json();
}

// Components:
<SubjectCard theta={0.45} thetaBand="B+" percentile={78} />
<ThetaTrendChart data={trendPoints} subject="math" />

- Tutor Dashboard (tutor_front):

export async function getTutorPriorities(subject: string, token: string) {
  return fetch(`/api/tutor/priorities?subject=${subject}&windowDays=14`, {
    headers: { Authorization: `Bearer ${token}` }
  });
}

// Table with sortable columns + "Assign Exam" buttons

- Parent Portal (parent_front):

async function downloadReport(childId: string, period: string) {
  const res = await fetch(`/api/parent/reports/${childId}/pdf?period=${period}`);
  const blob = await res.blob();
  saveAs(blob, `Report_${childId}_${period}.pdf`);
}

