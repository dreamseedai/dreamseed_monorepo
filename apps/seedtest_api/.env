# Database (use psycopg2 for local dev)
DATABASE_URL=postgresql+psycopg2://postgres:postgres@127.0.0.1:5432/seedtest_ci

# Auth: Bypass for development
LOCAL_DEV=true

# Optional: JWT settings (uncomment for production-like testing)
# JWKS_URL=https://your-auth/.well-known/jwks.json
# JWT_ISS=https://your-auth/
# JWT_AUD=seedtest-api

# --- Metrics and IRT ---
# Enable theta-based improvement index (I_t)
METRICS_USE_IRT_THETA=true

# R plumber IRT service (set when service is available)
# R_IRT_BASE_URL=http://r-irt-plumber.default.svc.cluster.local:8000
# R_IRT_INTERNAL_TOKEN=change-me
# R_IRT_TIMEOUT_SECS=20
