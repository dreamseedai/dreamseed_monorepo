{
  "venvPath": ".",
  "venv": ".venv",
  "typeCheckingMode": "basic",
  "pythonVersion": "3.11",
  "reportMissingImports": true,
  "exclude": [
    "**/node_modules",
    "**/dist",
    "**/.venv",
    "**/__pycache__",
    "**/.mypy_cache",
    "portal_front"
  ],
  "executionEnvironments": [
    { "root": "./backend" },
    { "root": "./shared" },
    { "root": "./apps/seedtest_api" },
    { "root": "." }
  ]
}
