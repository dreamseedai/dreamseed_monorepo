{
  "search.exclude": {
    "**/node_modules/**": true,
    "**/.git/**": true,
    "**/.next/**": true,
    "**/dist/**": true,
    "**/build/**": true,
    "**/.venv/**": true,
    "**/venv/**": true,
    "**/db_env/**": true,
    "**/mathml_env/**": true,
    "**/__pycache__/**": true,
    "**/.pytest_cache/**": true,
    "**/.mypy_cache/**": true,
    "**/.ruff_cache/**": true,
    "archive/**": true,
    "webtests/**": true,
    "translator.py/**": true,
    "monitoring/**": true,
    "Caddyfile/**": true,
    "_archive/**": true,
    "backup/**": true,
    "**/*.log": true,
    "**/*.db": true
  },
  "files.exclude": {
    "**/.git": true,
    "**/.DS_Store": true,
    "**/__pycache__": true,
    "**/.pytest_cache": true,
    "**/.mypy_cache": true,
    "**/*.pyc": true
  },
  "files.watcherExclude": {
    "**/.git/objects/**": true,
    "**/node_modules/**": true,
    "**/.venv/**": true,
    "**/venv/**": true,
    "**/db_env/**": true,
    "**/mathml_env/**": true,
    "**/dist/**": true,
    "**/build/**": true,
    "**/.mamba/**": true,
    "archive/**": true,
    "_archive/**": true
  },
  "github.copilot.advanced": {
    "excludedDirectories": [
      "node_modules", ".venv", "venv", "db_env", "mathml_env",
      ".git", ".history", "__pycache__", ".pytest_cache", 
      ".mypy_cache", ".ruff_cache", "dist", "build", ".mamba",
      "archive", "webtests", "translator.py", "monitoring", "_archive"
    ]
  },
  "npm.packageManager": "pnpm",
  "python.defaultInterpreterPath": "${workspaceFolder}/.venv/bin/python",
  "yaml.schemaStore.enable": false,
  "yaml.customTags": [
    "!And",
    "!If",
    "!Not",
    "!Or",
    "!Equals",
    "!Sub"
  ],
  "yaml.validate": false
}
